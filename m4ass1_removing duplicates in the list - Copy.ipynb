    "def remove(dup):\n",
    "    final_list=[]\n",
    "    for num in dup:\n",
    "        if num not in final_list:\n",
    "           final_list.append(num)\n",
    "    return final_list \n",
    "dup=[1,2,2,2,25,7,67,58,88]\n",
    "print(\"after removing the duplicate element in the list \",remove(dup))"
   ]
  }
